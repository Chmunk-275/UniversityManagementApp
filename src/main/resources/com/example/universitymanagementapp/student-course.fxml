<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.universitymanagementapp.ui.CourseManagementUI.CourseManagementStudentUI"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="studentCourseTable" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="courseNameColumn" prefWidth="300.0" text="Course Name"/>
            <TableColumn fx:id="instructorColumn" prefWidth="300.0" text="Instructor"/>
        </columns>
    </TableView>
</AnchorPane>
